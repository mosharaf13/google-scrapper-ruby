<div id="<%= dom_id search_stat %>">
  <p>
    <strong><%= SearchStat.human_attribute_name(:keyword) %>:</strong>
    <%= search_stat.keyword %>
  </p>

  <p>
    <strong><%= SearchStat.human_attribute_name(:ad_count) %>:</strong>
    <%= search_stat.ad_count %>
